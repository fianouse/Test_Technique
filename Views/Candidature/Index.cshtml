@model IEnumerable<last_test_tech.Models.Candidat>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Id)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.nom)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.prenom)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.mail)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.tele)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.niveau_detude)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.nbr_anne_exp)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.dn_employeur)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.nom)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.prenom)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.mail)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.tele)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.niveau_detude)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.nbr_anne_exp)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.dn_employeur)
            </td>
            <td>

                <form asp-action="Delete" asp-route-id="@item.Id" method="post">
                    <button type="submit">Delete</button>
                </form>
            </td>
        </tr>
}
    </tbody>
</table>
